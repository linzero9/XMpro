<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd" >
<struts >
  <package name="dict" namespace="/dict" extends="default" >
    <action name="eosDictEntryAction_*" class="eosDictEntryAction" method="{1}" >
      <result name="dictList" >
        /jsp/dict/dictList.jsp
      </result>
      <result name="toInsert">
       /jsp/dict/toInsert.jsp
      </result>
      <result name="toUpdate">
       /jsp/dict/toUpdate.jsp
      </result>
     <result name="toUpdateName">
       /jsp/dict/toUpdateName.jsp
      </result>
    </action>
  </package>
</struts>