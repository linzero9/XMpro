<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd" >
<struts >
  <package name="peopleManagement" namespace="/" extends="struts-default" >
    <action name="omInformationAction_*" class="omInformationAction" method="{1}" >
      <result name="queryOmInformationList" >
        /jsp/PeopleManagement/ominformation_list.jsp
      </result>
      <result name="queryOrgName" >
        /jsp/PeopleManagement/ominformation_orgNameList.jsp
      </result>
      <result name="queryRoleName" >
        /jsp/PeopleManagement/ominformation_roleNameList.jsp
      </result>
      <result name="queryPosiName" >
        /jsp/PeopleManagement/ominformation_posiNameList.jsp
      </result>
      
           
      
    </action>
  </package>
</struts>